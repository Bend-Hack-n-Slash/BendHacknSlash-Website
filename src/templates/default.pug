doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, maximum-scale=1")
    title #{site.title} - #{title}
    include scripts.pug
    include stylesheets.pug
  body
    include header.pug
    include menu.pug
    
    div(class="content")
        !=contents

    include footer.pug
